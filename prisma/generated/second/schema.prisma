// prisma/multi/second.schema.prisma

generator client {
  provider = "prisma-client-js"
  output   = "../generated/second" // custom output folder
}

datasource db2 {
  provider = "postgresql" // or mysql/sqlite
  url      = env("DATABASE_URL_2")
}

model Item {
  id   Int    @id @default(autoincrement())
  name String
}
